meta {
  name: Get Server Channels
  type: graphql
  seq: 14
}

post {
  url: {{URL}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{JWT}}
}

body:graphql {
  query {
    serverChannels(server_id: 1) {
      id
      name
      type
      topic
      position
      created_at
      messages {
        id
        content
        author {
          username
        }
      }
    }
  }
}
