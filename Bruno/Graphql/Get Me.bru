meta {
  name: Get Me
  type: graphql
  seq: 3
}

post {
  url: {{URL}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{JWT}}
}

body:graphql {
  query {
    me {
      id
      username
      email
      avatar_url
      created_at
      last_online_at
      status {
        status
        custom_status
        updated_at
      }
      servers {
        id
        name
        icon_url
      }
    }
  }
}
