meta {
  name: Get User By Username
  type: graphql
  seq: 5
}

post {
  url: {{URL}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{JWT}}
}

body:graphql {
  query {
    userByUsername(username: "convoke") {
      id
      username
      email
      avatar_url
      created_at
      status {
        status
        custom_status
      }
    }
  }
}
