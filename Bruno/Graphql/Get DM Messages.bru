meta {
  name: Get DM Messages
  type: graphql
  seq: 33
}

post {
  url: {{URL}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{JWT}}
}

body:graphql {
  query {
    dmMessages(dm_channel_id: 1, limit: 50, offset: 0) {
      id
      content
      created_at
      edited_at
      author {
        id
        username
        avatar_url
      }
    }
  }
}
